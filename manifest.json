{
  "name": "Slearch",
  "version": "1.1.2",
  "manifest_version": 2,
  "description": "Adds a shortcut to any web page which opens the first available search field by typing the \"/\" key",
  "icons": {
    "16": "slearch16.png",
    "48": "slearch48.png",
    "128": "slearch128.png"
  },
  "permissions": ["activeTab"],
  "content_scripts" : [{
    "matches": ["*://*/*"],
    "match_about_blank": true,
    "all_frames": true,
    "js": ["slearch.js"]
  }]
}