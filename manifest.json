{
  "name": "Slearch",
  "version": "0.0.1",
  "manifest_version": 1,
  "description": "Adds a shortcut to any web page which opens the first available search field by typing the \"/\" key",
  "icons": {
    "48": "slearch48.png",
    "120": "slearch120.png"
  },
  "permissions": ["activeTab"],
  "content_scripts" : [{
    "matches": ["*://*/*"],
    "match_about_blank": true,
    "all_frames": true,
    "js": ["slearch.js"]
  }]
}